nimations.scss":"@keyframes moveInLeft {\n  0% {\n    opacity: 0;\n    transform: translateX(-10rem);\n  }\n\n  80% {\n    transform: translateX(1rem);\n  }\n\n  100% {\n    opacity: 1;\n    transform: translateX(0);\n  }\n}\n\n@keyframes moveInRight {\n  0% {\n    opacity: 0;\n    transform: translateX(10rem);\n  }\n\n  80% {\n    transform: translateX(-1rem);\n  }\n\n  100% {\n    opacity: 1;\n    transform: translateX(0);\n  }\n}\n\n@keyframes moveInBottom {\n  0% {\n    opacity: 0;\n    transform: translateY(10rem);\n  }\n\n  100% {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n","sass/base/_base.scss":"@use \"../abstracts\" as *;\n\nhtml {\n\tfont-size: 62.5%; //resets 1 rem to 10px\n}\n\nbody {\n\tfont-family: $font-family;\n\tfont-size: $font-size;\n\tbackground: $main-back;\n\tposition: relative;\n\tpadding-bottom: 50rem;\n\t@include respond(mobile) {\n\t\tbackground-color: red;\n\t}\n}\n","sass/abstracts/_variables.scss":"//Colors\n$moderate-cyan: hsl(176, 50%, 47%);\n$dark-cyan: hsl(176, 72%, 28%);\n$black: hsl(0, 0%, 0%);\n$dark-gray: hsl(0, 0%, 48%);\n$main-back: rgb(250, 250, 250);\n\n$font-size: 1.6rem;\n\n$font-family: \"Commissioner\", sans-serif;\n$font-weight-thin: 400;\n$font-weight-med: 500;\n$font-weight-thick: 700;\n","sass/abstracts/_mixins.scss":"@mixin clearfix {\n\t&::after {\n\t\tcontent: \"\";\n\t\tdisplay: table;\n\t\tclear: both;\n\t}\n}\n\n@mixin center_horizontally_and_vertically {\n\tposition: absolute;\n\ttop: 50%;\n\tleft: 50%;\n\ttransform: translate(-50%, -50%);\n}\n\n@mixin respond($breakpoint) {\n\t@if $breakpoint==mobile {\n\t\t@media only screen and (max-width: 23.44em) {\n\t\t\t// 375px\n\t\t\t@content;\n\t\t}\n\t}\n\t@if $breakpoint==desktop {\n\t\t@media only screen and (max-width: 90em) {\n\t\t\t// 1440px\n\t\t\t@content;\n\t\t}\n\t}\n}\n","sass/base/_resets.scss":"/* Box sizing rules */\n*,\n*::before,\n*::after {\n\tbox-sizing: border-box;\n}\n\n/* Remove default margin */\nbody,\nh1,\nh2,\nh3,\nh4,\np,\nfigure,\nblockquote,\ndl,\ndd {\n\tmargin: 0;\n}\n\n/* Remove list styles on ul, ol elements with a list role, which suggests default styling will be removed */\nul[role=\"list\"],\nol[role=\"list\"] {\n\tlist-style: none;\n}\n\n/* Set core root defaults */\nhtml:focus-within {\n\tscroll-behavior: smooth;\n}\n\n/* Set core body defaults */\nbody {\n\tmin-height: 100vh;\n\ttext-rendering: optimizeSpeed;\n\tline-height: 1.5;\n}\n\n/* A elements that don't have a class get default styles */\na:not([class]) {\n\ttext-decoration-skip-ink: auto;\n}\n\n/* Make images easier to work with */\nimg,\npicture {\n\tmax-width: 100%;\n\tdisplay: block;\n}\n\n/* Inherit fonts for inputs and buttons */\ninput,\nbutton,\ntextarea,\nselect {\n\tfont: inherit;\n}\n\n/* Remove all animations, transitions and smooth scroll for people that prefer not to see them */\n@media (prefers-reduced-motion: reduce) {\n\thtml:focus-within {\n\t\tscroll-behavior: auto;\n\t}\n\n\t*,\n\t*::before,\n\t*::after {\n\t\tanimation-duration: 0.01ms !important;\n\t\tanimation-iteration-count: 1 !important;\n\t\ttransition-duration: 0.01ms !important;\n\t\tscroll-behavior: auto !important;\n\t}\n}\n\n//https://piccalil.li/blog/a-modern-css-reset\n","sass/base/_utilities.scss":".u-center-text {\n\ttext-align: center !important;\n}\n","sass/components/_modal.scss":".modal {\n\tdisplay: none;\n}\n","sass/layout/_header.scss":"@use \"../abstracts\" as *;\n\n.header {\n\twidth: 100vw;\n\theight: 46vh;\n\tbackground-image: url(\"/images/image-hero-desktop.jpg\");\n\n\t&_box {\n\t\theight: 35%;\n\t\twidth: 100%;\n\t\t// background-color: red;\n\n\t\tdisplay: flex;\n\t\tjustify-content: space-between;\n\t\talign-items: center;\n\n\t\t&__image {\n\t\t\tmargin-left: 10vw;\n\n\t\t\timg {\n\t\t\t\ttransform: scale(1.5);\n\t\t\t}\n\t\t}\n\n\t\tul {\n\t\t\tdisplay: flex;\n\t\t\tlist-style: none;\n\t\t\tmargin-right: 8vw;\n\n\t\t\tli {\n\t\t\t\tpadding: 0 2.5rem;\n\t\t\t\tcolor: $main-back;\n\t\t\t\tfont-size: 2rem;\n\t\t\t\tfont-weight: $font-weight-thick;\n\t\t\t\twhite-space: nowrap;\n\t\t\t\ttransition: 0.5s ease;\n\n\t\t\t\t&:hover {\n\t\t\t\t\tcolor: $dark-cyan;\n\t\t\t\t\tcursor: pointer;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n}\n","sass/layout/_main_content.scss":".main_content {\n\tposition: absolute;\n\ttop: 38vh;\n\tleft: 50%;\n\ttransform: translateX(-50%);\n\n\twidth: 50vw;\n\tpadding: 2rem;\n\n\tbackground-color: white;\n\tbox-shadow: 0 5px 15px 9px rgba(0, 0, 0, 0.07);\n\n\t&:after {\n\t\tcontent: \"\";\n\t\tposition: relative;\n\t\theight: 200rem;\n\t\tbackground-color: red;\n\t\twidth: 50%;\n\t}\n}\n\nbody {\n\tpadding-bottom: 5rem;\n\toverflow: scroll;\n}\n"},"lineCount":null}},"error":null,"hash":"27e828bc55947c2e2e089276ab895c87","cacheData":{"env":{}}}